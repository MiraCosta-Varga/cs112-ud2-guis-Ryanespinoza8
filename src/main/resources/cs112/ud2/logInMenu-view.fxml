<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<StackPane xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs112.ud2.controllers.LogInMenuController">
   <children>
      <AnchorPane>
         <VBox alignment="CENTER" prefHeight="400.0" prefWidth="219.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
               <Label alignment="CENTER" text="MOVIE PICKER">
                  <font>
                     <Font size="30.0" />
                  </font>
               </Label>
               <VBox alignment="CENTER" spacing="20.0">
                  <children>
                     <Button fx:id="adminLoginButton" mnemonicParsing="false" onAction="#onAdminLoginButtonClicked" prefHeight="45.0" prefWidth="166.0" text="Admin Login">
                        <font>
                           <Font size="20.0" />
                        </font>
                     </Button>
                     <Button fx:id="regularLoginButton" mnemonicParsing="false" onAction="#onRegularLoginButtonClicked" prefHeight="45.0" prefWidth="166.0" text="Regular Login">
                        <font>
                           <Font size="20.0" />
                        </font>
                     </Button>
                     <Button fx:id="quitButton" mnemonicParsing="false" onAction="#onQuitButtonClicked" prefHeight="45.0" prefWidth="100.0" text="QUIT">
                        <font>
                           <Font size="20.0" />
                        </font>
                     </Button>
                  </children>
               </VBox>
            </children>
         </VBox>
      </AnchorPane>
   </children>
</StackPane>
